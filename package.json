{
  "name": "coltigent-website",
  "version": "1.0.0",
  "homepage": "https://coltigent.github.io",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.7",
    "emailjs-com": "^3.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.8.1",
    "aos": "^2.3.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "dev": "react-scripts start",
    "serve": "npx serve -s build -l 3000",
    "test:build": "npm run build && npx serve -s build -l 3000",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "check-deps": "npx depcheck",
    "security-audit": "npm audit --audit-level=moderate",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\""
  },
  "devDependencies": {
    "gh-pages": "^6.1.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "coltigent",
    "technology",
    "software",
    "consulting",
    "web-development",
    "cloud-computing",
    "data-engineering",
    "ai-ml",
    "startup"
  ],
  "author": "Coltigent Technology Services Pvt Ltd",
  "description": "Coltigent Technology Services - Leading IT consulting, software development, and digital transformation solutions. Specializing in web development, cloud computing, data engineering, and AI/ML integration.",
  "license": "MIT"
}
